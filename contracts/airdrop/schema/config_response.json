{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigResponse",
  "type": "object",
  "required": [
    "are_claims_allowed",
    "astro_token_address",
    "auction_contract_address",
    "from_timestamp",
    "merkle_roots",
    "owner",
    "to_timestamp"
  ],
  "properties": {
    "are_claims_allowed": {
      "type": "boolean"
    },
    "astro_token_address": {
      "type": "string"
    },
    "auction_contract_address": {
      "type": "string"
    },
    "from_timestamp": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "merkle_roots": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "owner": {
      "type": "string"
    },
    "to_timestamp": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
